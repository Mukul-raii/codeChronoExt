{
  "name": "miss-minutes",
  "displayName": "Miss-Minutes - Time Tracker",
  "description": "Automatic coding time tracker with git integration and analytics",
  "version": "0.0.4",
  "publisher": "Mukulrai",
  "engines": {
    "vscode": "^1.80.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mukul-raii/CodeChrono.git"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "time tracking",
    "productivity",
    "analytics",
    "git",
    "coding stats"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:miss-minutes.setApiKey",
    "onCommand:miss-minutes.helloWorld"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "miss-minutes.setApiKey",
        "title": "Miss-Minutes: Set API Key"
      },
      {
        "command": "miss-minutes.helloWorld",
        "title": "Miss-Minutes: Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack && node scripts/copy-native-modules.js",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map && node scripts/copy-native-modules.js",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^18.19.130",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.6.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "sqlite3": "^5.1.6"
  }
}
